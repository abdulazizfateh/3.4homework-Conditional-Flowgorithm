    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="num" type="Real" array="False" size=""/>
            <output expression="&quot;Son kiriting&quot;" newline="True"/>
            <input variable="num"/>
            <if expression="num == 0">
                <then>
                    <assign variable="num" expression="10"/>
                </then>
                <else>
                    <if expression="num &gt; 0">
                        <then>
                            <assign variable="num" expression="num + 1"/>
                        </then>
                        <else>
                            <assign variable="num" expression="num - 2"/>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="num" newline="True"/>
        </body>
